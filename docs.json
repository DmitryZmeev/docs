{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "Neterial Documentation",
  "colors": {
    "primary": "#546BE4",
    "light": "#546BE4",
    "dark": "#546BE4"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "getting-started",
              "development"
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Neterial Website",
          "href": "https://neterial.io",
          "icon": "globe"
        },
        {
          "anchor": "Blog",
          "href": "https://neterial.io/blog",
          "icon": "newspaper"
        },
        {
          "anchor": "Support",
          "href": "https://neterial.io/contact",
          "icon": "headset"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Sign-Up",
        "href": "https://console.neterial.io"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Sign-In",
      "href": "https://console.neterial.io"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/neterialio",
      "linkedin": "https://linkedin.com/company/neterial"
    }
  }
}
